pragma solidity ^0.8.0;

contract SimpleStorage {
    uint private storedData;

    function set(uint x) public {
        storedData = x;
    }

    function get() public view returns (uint) {
        return storedData;
    }

    function increment() public {
        storedData += 1; 
    }

    function decrement() public {
        storedData -= 1; 
    }
}
